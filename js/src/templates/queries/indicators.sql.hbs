 with d as (
 select * from indicator_data where iso='{{iso}}'
 )
 SELECT
   c.short_name,
   c.score_range,
   c.desired_direction,
   c.has_historical_info,
   c.max_score,
   c.methodology_link,
   c.min_score,
   c.product_description,
   c.product_logo,
   c.product_name,
   c.units,
   c.units_abbr,
   c.organization,
   '{{iso}}' as iso,
   d.notes,
   d.score,
   d.score_text,
   d.year
  FROM indicator_config AS c
    left JOIN  d ON c.short_name = d.short_name
  ORDER BY c.short_name asc, d.year asc
