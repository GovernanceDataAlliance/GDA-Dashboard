SELECT
  c.desired_direction,
  c.has_historical_info,
  c.max_score,
  c.methodology_link,
  c.min_score,
  c.product_description,
  c.product_logo,
  c.product_name,
  c.score_range,
  c.short_name,
  c.units,
  d.iso,
  d.notes,
  d.score,
  d.score_text,
  d.year
 FROM {{table}} AS d
   INNER JOIN indicator_config AS c ON d.short_name = c.short_name
 WHERE d.iso = '{{iso}}' AND d.year = '{{year}}'
