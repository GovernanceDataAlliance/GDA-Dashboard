with d as (
select * from indicator_data where 
{{{countriesAndYears}}}
)

SELECT 
  distinct on(c.product_name, d.iso) c.product_name,
  c.score_range,
  c.desired_direction,
  c.has_historical_info,
  c.max_score,
  c.methodology_link,
  c.min_score,
  c.product_description,
  c.product_logo,
  c.units,
  c.units_abbr,
  c.organization,
  c.short_name,
  d.iso,
  d.notes,
  d.score,
  d.score_text,
  d.year 
 FROM indicator_config AS c 
   right JOIN  d 
ON c.short_name = d.short_name 
order by d.iso, c.product_name
