{{#if data}}
  <ul>
    {{#each data}}
      <li class="search-area{{#if selected}} selected{{/if}}" title="{{title}}" data-iso="{{iso3}}"><a href="/GDA-Dashboard/countries#{{iso3}}"><span class="name">{{{name}}}</span></a></li>
    {{/each}}
  </ul>
{{else}}
  <div class="no-results">No results</div>
{{/if}}


