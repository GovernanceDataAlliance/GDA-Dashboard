<div class="l-toolbar -year-selector">
  <div class="wrap">
    <div class="m-compare-selectors -year">
      <p class="is-small-text">Select date to compare the country data</p>
      <div class="selector-wrapper -year">
        <div class="btn-drop-down">
          <div class="js--year-selector-compare"></div>
        </div>
      </div>
    </div>
  </div>
</div>

<div class="js--compare-selectors"></div>

<div class="l-content -fullscreen">
  <div class="wrap">
    <div class="m-comparison-table">
      <div class="js--comparison-indicators -index"></div>
      <div class="js--country-1 -score"></div>
      <div class="js--country-2 -score"></div>
      <div class="js--country-3 -score"></div>
    </div>
    <div class="m-advise -compare">
      <p>Please, select the countries that you would like to compare into the selects above this message</p>
    </div>
  </div>
</div>

<div class="js--compare-toolbar">
  <div class="l-toolbar -footer">
    <div class="wrap">
      <div class="m-legend">
        <ul>
          <li class="-best"><span class="legend-title">The best</span></li>
          <li class="-good"><span class="legend-title">Good</span></li>
          <li class="-medium"><span class="legend-title">Medium</span></li>
          <li class="-regular"><span class="legend-title">Regular</span></li>
          <li class="-bad"><span class="legend-title">Bad</span></li>
        </ul>
      </div>
      <div class="js--toolbar-utils"></div>
    </div>
  </div>
</div>

