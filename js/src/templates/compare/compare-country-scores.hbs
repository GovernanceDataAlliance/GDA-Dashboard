<ul class="{{iso}}">
  {{#each scores}}
    {{#if score}}
      <li class="table-cell {{short_name}} {{year}} {{#if classColor}} {{classColor}} {{/if}}">
      {{#round}}{{score}}{{/round}}
      </li>
    {{else}}
      <li class="table-cell no-data {{short_name}}">no data</li>
    {{/if}}
  {{/each}}
</ul>
