<ul class="table-header">
  {{#each indicators}}
    <li class="table-cell {{short_name}}">
      <h3>{{product_name}}</h3>
      <div class="organization"> 
        <h2>{{organization}}</h2>
        {{#if product_description}}
          <button class="btn-info" data-info="{{product_description}}">i</button>
        {{/if}}
      </div>
    </li>
  {{/each}}
</ul>